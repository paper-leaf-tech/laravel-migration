{
  "name": "paper-leaf-tech/laravel-migration",
  "description": "A package to manage Laravel Migrations",
  "license": "MIT",
  "authors": [
    {
      "name": "Brendan Angerman",
      "email": "brendan@paper-leaf.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "spatie/laravel-package-tools": "^1.92"
  },
  "autoload": {
    "psr-4": {
      "PaperleafTech\\LaravelMigration\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PaperleafTech\\LaravelMigration\\LaravelMigrationServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.24"
  }
}
